{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",

  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "document": true,
    "localStorage": true,
    "window": true,
    "ga": true
  },
  "rules": {
    "quote-props": "off",
    "jsx-a11y/href-no-hash": "off"
  }
}
